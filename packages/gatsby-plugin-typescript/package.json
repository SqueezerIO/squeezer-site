{
  "name": "gatsby-plugin-typescript",
  "version": "1.4.8",
  "description": "Adds TypeScript support for Gatsby layouts and pages.",
  "author": "Kyle Mathews <mathews.kyle@gmail.com>",
  "contributors": [
    "Noah Lange <noahrlange@gmail.com>"
  ],
  "dependencies": {
    "babel-runtime": "^6.26.0",
